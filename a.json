{
    "import": {
        "bin": [
            {
                "name": "payload",
                "bin": "target/release/libpayload.so"
            }
        ]
    },
    "pipeline": [
        {
            "ref": "my",
            "module": "payload",
            "tags": {
                "before": [
                    "hello"
                ]
            },
            "params": {
                "content": "my"
            }
        },
        {
            "tags": {
                "produce": []
            },
            "module": "payload",
            "ref": "hello",
            "params": {
                "content": "Hello"
            }
        },
        {
            "params": {
                "content": "brother"
            },
            "module": "payload",
            "ref": "brother"
        }
    ]
}