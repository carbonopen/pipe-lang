import {
    mod "../print" ()

    mod "../helpers/concat" ()

     bin "../../../target/release/liblink.so" (
        name="link"
    )

}

args {
    first_name: String = "default"
    last_name: String = "test"
}

pipeline {
    link {}

    print {
        text=`First name: ${args.first_name}`
    }

    print {
        text=`Last name: ${args.last_name}`
    }

    print {
        text="------------"
    }


}

