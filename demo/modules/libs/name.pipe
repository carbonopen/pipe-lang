import {
    bin "../../../target/release/libprint.so" (
        name="print"
    )
     bin "../../../target/release/liblink.so" (
        name="link"
    )
    mod "../helpers/concat" ()
}

args {
    first_name: String
    last_name: String
}

pipeline {
    print {
        text="Your"
    }

    print {
        text="name"
    }

    print {
        text="is"
    }

    concat {
        item1=${args.first_name}
        item2=${args.last_name}
    }
}


